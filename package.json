{
  "name": "artistic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/poppins": "^4.5.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.8.4",
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
